<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodOrder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomeBudgetAppfinalapp">
        <activity android:name="com.example.foodOrder.viewOrder"></activity>
        <activity android:name="com.example.foodOrder.orderDetail" />
        <activity android:name="com.example.foodOrder.addPerson" />
        <activity android:name="com.example.foodOrder.tableOrder" />
        <activity android:name="com.example.foodOrder.Dashboard" />
        <activity android:name="com.example.foodOrder.splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.foodOrder.MainActivity" />
    </application>

</manifest>